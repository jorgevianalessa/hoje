            node3.setAttribute("onclick", "buscLojista(" + "'" + codigo + "'" + ")");
            document.getElementById(index).appendChild(node3);
        }

        let referencia = Produto.referencia;
        let idrefer = "idrefer" + i;
        criaContaneinerReferencia();

