<style>
  * {box-sizing: border-box}
  body {font-family: Verdana, sans-serif; margin:0}

  /* Slideshow container */
  .slideshow-container {
    position: relative;
    background: #ffffff;
  }

  /* Slides => Primeira Painel */
  .SlidesPainelH {
    display: none;
    padding: 50px;
    text-align: center;
    background-color: #ffffff;
  }

  .nextHum {
    position: absolute;
    right: 0;
    border-radius: 3px 0 0 3px;
  }

  .prevHum, .nextHum {
    cursor: pointer;
    position: absolute;
    top: 50%;
    width: auto;
    margin-top: -30px;
    padding: 16px;
    color: #888;
    font-weight: bold;
    font-size: 20px;
    border-radius: 0 3px 3px 0;
    user-select: none;
  }

  .dotHum{
      cursor: pointer;
      height: 15px;
      width: 15px;
      margin: 0 2px;
      background-color: rgb(9, 135, 194);
      border-radius: 50%;
      display: inline-block;
      transition: background-color 0.6s ease;
  }

  /* Next & previous buttons */
  .SlidesPainelD{
    display: none;
    padding: 50px;
    text-align: center;
    background-color: #ffffff;
  }

  .prevDois, .nextDois {
      cursor: pointer;
      position: absolute;
      top:50%;
      width: auto;
      margin-top: -30px;
      padding: 16px;
      color: #888;
      font-weight: bold;
      font-size: 20px;
      border-radius: 0 3px 3px 0;
      user-select: none;
  }

  /* Position the "next button" to the right */
  .nextDois{
    position: absolute; 
    right: 0;
    border-radius: 3px 0 0 3px;
  }
 
  .dotDois{
    cursor: pointer;
    height: 15px;
    width: 15px;
    margin: 0 2px;
    background-color: #c4c4c1;
    border-radius: 50%;
    display: inline-block;
    transition: background-color 0.6s ease;
  }

  /* On hover, add a black background color with a little bit see-through */
  .prev:hover, .next:hover {
    background-color: rgba(0,0,0,0.8);
    color: white;
  }

  .prev1:hover, .next1:hover {
    background-color: rgba(0,0,0,0.8);
    color: white;
  }

  /* The dot/bullet/indicator container */
 .dot-container{
    text-align: center;
    padding: 10px;
    background: #ddd;
  }

  .dot-container1{
    text-align: center;
    padding: 5px;
    background: #f3f1f1;
  }

  /* The dots/bullets/indicators */
  .seta1{
    cursor: pointer;
    height: 15px;
    width: 15px;
    margin: 0 2px;
    background-color: rgb(9, 135, 194);
    border-radius: 50%;
    display: inline-block;
    transition: background-color 0.6s ease;
  }

  /* Add a background color to the active dot/circle */
  .active, .dot:hover {
    background-color: #ffffff;
  }

  /* Add an italic font style to all quotes */
  q {font-style: italic;}

  /* Add a blue color to the author */
  .author {color: cornflowerblue;}
  /*PAINEL 2 - MEIO/LADO ESQUERDO::::::::::::::::::::::::::::::::::::::::::::::::::::*/
